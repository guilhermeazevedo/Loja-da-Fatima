	<field name="COD_OPERACAO_PRODUTOS_COMPRA_VENDA" class="java.lang.Integer"/>
	<field name="DS_PRODUTO" class="java.lang.String"/>
	<field name="QT_PRODUTO" class="java.math.BigDecimal"/>
