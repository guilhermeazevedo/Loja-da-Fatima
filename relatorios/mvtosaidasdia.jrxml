	<queryString language="SQL">
		<![CDATA[SELECT "CD_MVTO",
                       "DS_MVTO",
                       TO_CHAR("DT_MVTO", 'DD/MM/YYYY ÀS HH24:MI:SS'),
                       "VL_ANTES",
                       "VL_ATUAL",
